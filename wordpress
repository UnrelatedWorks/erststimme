<!-- Wordpress side implementation that gets the newest github einbetten. Wordpress does not need to be touched -->
<!DOCTYPE html>
<html>
<head>
    <title>ErststimmeImNetz</title>
</head>
<body>
    <div id="github-content"></div>

    <script>
        //Der code Ã¤ndert sich, das magazin bleibt gleich!
        fetch('https://raw.githubusercontent.com/UnrelatedWorks/erststimme/main/einbetten2.html')
            .then(response => response.text())
            .then(htmlContent => {
                // Insert the HTML content into the 'github-content' div
                document.getElementById('github-content').innerHTML = htmlContent;
            })
            .catch(error => {
                console.error('Error fetching GitHub HTML:', error);
            });
    </script>
</body>
</html>
